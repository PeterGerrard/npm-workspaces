{
  "name": "@plg/npm-workspace",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "name": "@plg/npm-workspace",
      "workspaces": [
        "./packages/*"
      ]
    },
    "node_modules/@plg/components": {
      "resolved": "packages/components",
      "link": true
    },
    "node_modules/@plg/docsite": {
      "resolved": "packages/docsite",
      "link": true
    },
    "packages/components": {
      "name": "@plg/components",
      "version": "0.1.0",
      "license": "ISC"
    },
    "packages/docsite": {
      "name": "@plg/docsite",
      "version": "0.1.0",
      "license": "ISC",
      "dependencies": {
        "@plg/components": "^0.1.0"
      }
    }
  },
  "dependencies": {
    "@plg/components": {
      "version": "file:packages/components"
    },
    "@plg/docsite": {
      "version": "file:packages/docsite",
      "requires": {
        "@plg/components": "^0.1.0"
      }
    }
  }
}
